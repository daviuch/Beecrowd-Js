var input = require('fs').readFileSync('/dev/stdin', 'utf8');
var lines = input.split('\n');


 var p = parseInt(lines.shift())
  var num = 0
  var ndois = 1
  var text = ""
  var mud = num + ndois

  for (var x = 1; x <= p; x++) {

    text += num


    if (x < p) {
      text += " "
    }

    num = ndois
    ndois = mud
    mud = num + ndois

  }

  console.log(text)
